<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Dictionary" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/turquoise_500</item>
        <item name="colorPrimaryVariant">@color/turquoise_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/lime_500</item>
        <item name="colorSecondaryVariant">@color/lime_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowSharedElementsUseOverlay">false</item>
    </style>
</resources>